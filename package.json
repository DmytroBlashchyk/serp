    "@types/redis": "^2.8.29",
    "@types/request-promise": "^4.1.48",
    "@types/supertest": "^2.0.10",
    "@types/valid-url": "^1.0.7",
    "@types/validator": "^13.7.17",
    "@types/yargs": "^17.0.7",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
