    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "connect-redis": "^6.0.0",
    "crypto-js": "^4.2.0",
    "dotenv": "^8.6.0",
    "google-ads-api": "^16.0.0-rest-beta3",
    "ioredis": "^4.28.5",
    "is-reachable": "^5.2.1",
    "joi": "^17.9.2",
    "json-2-csv": "^3.17.0",
    "language-name-map": "^0.3.0",
    "moment": "^2.29.1",
    "moment-timezone": "^0.5.45",
    "nanoid": "^3.1.23",
    "nestjs-command": "^3.1.0",
    "nestjs-form-data": "^1.4.3",
    "nestjs-postmark": "^1.0.2",
    "nestjs-redis": "^1.3.3",
    "nestjs-typeorm-paginate": "^2.5.3",
    "newrelic": "^11.18.0",
    "passport-google-oauth20": "^2.0.0",
    "pg": "^8.6.0",
    "proper-url-join": "^2.1.1",
    "rate-limiter-flexible": "^5.0.3",
    "redis": "^3.1.2",
    "reflect-metadata": "^0.1.13",
    "request": "^2.88.2",
