{
  "name": "serpnest-backend",
  "private": true,
  "version": "1.0.0",
  "description": "Nest TypeScript starter repository",
  "license": "MIT",
  "scripts": {
    "build": "nest build serpnest-api",
    "build:queue": "nest build serpnest-queue",
    "type-check": "tsc --pretty --noEmit",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start serpnest-api --watch",
    "start:debug": "nest start serpnest-api --debug --watch",
    "start:prod": "node dist/src/main",
    "cli:emit:run-hourly-batch-check": " nestjs-command emit:run-hourly-batch-check",
    "queue:dev": "IS_QUEUE=True nest start serpnest-queue --watch",
    "fastify:dev": "nest start serpnest-fastify-api --watch",
    "queue:prod": "IS_QUEUE=True node dist-queue/src/queue-entrypoint.js",
    "lint": "eslint \"src/modules/**/*.ts\" --ignore-pattern \"/src/modules/db/migrations/*\"",
    "emit:update-locations-for-google-serp": " nestjs-command emit:update-locations-for-google-serp",
    "emit:add-new-locations-for-google-serp": " nestjs-command emit:add-new-locations-for-google-serp",
    "emit:update-locations-for-google-keyword-data": " nestjs-command emit:update-locations-for-google-keyword-data",
    "emit:update-locations-for-bing-serp": " nestjs-command emit:update-locations-for-bing-serp",
    "emit:update-locations-for-bing-keyword-data": " nestjs-command emit:update-locations-for-bing-keyword-data",
    "emit:update-locations-for-youtube-serp": " nestjs-command emit:update-locations-for-youtube-serp",
    "emit:update-locations-for-yahoo-serp": " nestjs-command emit:update-locations-for-yahoo-serp",
    "emit:update-locations-for-baidu-serp": " nestjs-command emit:update-locations-for-baidu-serp",
    "emit:add-new-keyword-data-locations-for-google": " nestjs-command emit:add-new-keyword-data-locations-for-google",
    "emit:add-new-keyword-data-locations-for-bing": " nestjs-command emit:add-new-keyword-data-locations-for-bing",
    "emit:add-new-locations-for-youtube-serp": " nestjs-command emit:add-new-locations-for-youtube-serp",
    "emit:add-new-locations-for-yahoo-serp": " nestjs-command emit:add-new-locations-for-yahoo-serp",
    "emit:add-new-locations-for-bing-serp": " nestjs-command emit:add-new-locations-for-bing-serp",
    "emit:add-new-locations-for-baidu-serp": " nestjs-command emit:add-new-locations-for-baidu-serp",
    "emit:assign-a-limit-of-number-of-live-mode-updates-for-keywords-per-day-to-existing-accounts": " nestjs-command emit:assign-a-limit-of-number-of-live-mode-updates-for-keywords-per-day-to-existing-accounts",
    "test": "FORCE_COLOR=true NODE_NO_WARNINGS=1 node --no-deprecation node_modules/.bin/jest --silent=false --detectOpenHandles --forceExit",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "migration:generate": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:generate -n",
    "migration:create": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:create -n",
    "migration:run": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:run",
    "migration:revert": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:revert",
    "seed:create": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:create -c seeds -n",
    "seed:run": "node --stack-size=65500 -r ts-node/register -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:run -c seeds -t=false",
    "seed:revert": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:revert -c seeds",
    "db:init": "npm run migration:run && npm run seed:run",
    "prepare": "husky install",
    "pre-commit": "lint-staged"
  },
  "dependencies": {
    "@2toad/profanity": "^1.3.1",
    "@hapi/joi": "^17.1.1",
    "@nestjs-modules/mailer": "^1.8.1",
    "@nestjs/bull": "^0.4.2",
    "@nestjs/common": "^7.6.15",
    "@nestjs/config": "^0.6.3",
    "@nestjs/core": "^7.6.15",
